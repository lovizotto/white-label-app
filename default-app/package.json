{
  "name": "white-label-webapp",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "clientA": "file:../clientA",
    "clientB": "file:../clientB",
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.19",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.3.4",
    "chakra-ui-steps": "^2.0.4",
    "express": "^4.18.2",
    "framer-motion": "^10.9.1",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "../node_modules/.bin/react-scripts start",
    "start:clientA": "REACT_APP_BRAND=clientA react-scripts start",
    "start:clientB": "REACT_APP_BRAND=clientB react-scripts start",
    "build": "../node_modules/.bin/react-scripts build",
    "test": "../node_modules/.bin/react-scripts test",
    "eject": "../node_modules/.bin/react-scripts eject",
    "serve": "../node_modules/.bin/nodemon server/app.js",
    "build:clientA": "REACT_APP_CLIENT=clientA node ../config/loadConfig.js && react-scripts build",
    "build:clientB": "REACT_APP_CLIENT=clientB node ../config/loadConfig.js && react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
